{
    "manifest_version" : 2,
    "name": "Movie Extension",
    "description": "Open Site Automatically",
    "version": "1.1.2",
    "icons": {"128": "icon_128.png"},
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "storage",
        "activateTab"
    ],
    "options_ui": {
        "page": "options.html",
        "open_in_tab": false
    }
}